{
  "name": "hero-dashboard",
  "version": "0.3.1",
  "private": true,
  "repository": "tue-robotics/hero-dashboard",
  "author": {
    "name": "Matthijs van der Burgh",
    "email": "MatthijsBurgh@outlook.com",
    "url": "https://github.com/MatthijsBurgh"
  },
  "main": "main.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "bootstrap": "^4.3.1",
    "electron": "^4.1.4",
    "eventemitter2": "^4.1.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.6",
    "reactstrap": "^8.1.1",
    "react-dom": "^16.8.6",
    "react-scripts": "0.9.5",
    "roslib": "^1.0.1"
  },
  "devDependencies": {
    "dependency-check": "^4.1.0",
    "electron": "^4.1.4",
    "electron-packager": "^13.0.0",
    "rimraf": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf dist/",
    "deps": "npm run deps:missing && npm run deps:extra",
    "deps:missing": "dependency-check . --missing",
    "deps:extra": "dependency-check . --extra --no-dev",
    "prebuild": "npm run clean",
    "build": "react-scripts build",
    "postbuild": "npm run deps",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "deploy-linux": "electron-packager . hero-dashboard --overwrite --asar=true --platform=linux --arch=x64 ---prune=true --out=dist",
    "version": "npm run build",
    "patch": "npm version patch && npm run postpublish",
    "minor": "npm version minor && npm run postpublish",
    "major": "npm version major && npm run postpublish",
    "postpublish": "git push && git push --tags"
  },
  "homepage": "."
}
